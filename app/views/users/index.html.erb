<h1>Here is the list of all users</h1>
<ul>
  <% @users.each do |user| %>
    <%= link_to user_path(user) do%>
    <li>
        <%= image_tag user.photo %>
        <p><%= user.name %></p>
        # <p><%= user.bio %></p>
        <p>Number of posts : <%= user.posts.size %></p>
    </li>
    <% end %>
   <% end %>
    
</ul>

